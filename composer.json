{
    "name": "jenko/flysystem-gaufrette",
    "description": "Gaufrette adapter for Flysystem",
    "type": "library",
    "require": {
        "php": "^7.2 || ^8.0",
        "league/flysystem": "^2.0",
        "knplabs/gaufrette": "^0.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.4",
        "league/flysystem-adapter-test-utilities": "^2.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ian Jenkins",
            "email": "ian@jenkins-web.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jenko\\Flysystem\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jenko\\Flysystem\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "stable"
}
