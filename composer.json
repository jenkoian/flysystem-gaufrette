{
    "name": "jenko/flysystem-gaufrette",
    "description": "Gaufrette adapter for Flysystem",
    "type": "library",
    "require": {
        "php": ">=5.4.0",
        "league/flysystem": "^1.0",
        "knplabs/gaufrette": "^0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ian Jenkins",
            "email": "ian@jenkins-web.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jenko\\Flysystem\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "stable"
}
